{
  "name": "vos-command-macro",
  "displayName": "vos.command.macro",
  "description": "Vos Command Macro",
  "icon": "images/Icon.png",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "cm",
      "aliases": ["VOS Command Macro", "cm"],
      "extensions": [".cm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cm",
      "scopeName": "source.cm",
      "path": "./syntaxes/cm.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Vince1024/VOS-Command-Macro-Syntax-Highlighting-Support-For-VsCode"
  },
  "homepage": "https://github.com/Vince1024/VOS-Command-Macro-Syntax-Highlighting-Support-For-VsCode/blob/main/README.md",
  "publisher": "Vincent1024",
  "pricing": "Free"
}
